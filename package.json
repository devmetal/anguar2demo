{
  "name": "jsexamples2",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build & npm run lite",
    "build": "browserify ./src/main.js -o ./public/bundle.js",
    "lite": "lite-server --files './index.html' './public/**/*.js'"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-plugin-angular2-annotations": "^2.0.1",
    "babel-plugin-transform-async-to-generator": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babelify": "^7.2.0",
    "babylon": "^6.3.20",
    "browserify": "^12.0.1",
    "lite-server": "^1.3.1",
    "markdown": "^0.5.0",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.2",
    "angular2": "2.0.0-beta.0",
    "rxjs": "^5.0.0-beta.0",
    "systemjs": "0.19.6",
    "zone.js": "^0.5.8"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-1"
          ],
          "plugins": [
            "transform-async-to-generator"
          ]
        }
      ]
    ]
  }
}
